<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- shipping.trace.line action window -->
    <record id="shipping_trace_line_action" model="ir.actions.act_window">
        <field name="name">Movimientos de Despacho</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">shipping.trace.line</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'group_by' : ['partner_id','product_id']}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                <!-- Add Text Here -->
            </p><p>
                <!-- More details about what a user can do with this object will be OK --> 
            </p>
        </field>
    </record>

    <!-- shipping.trace.line tree view -->
    <record id="shipping_trace_line_view_tree" model="ir.ui.view">
        <field name="name">shipping.trace.line.view.tree</field>
        <field name="model">shipping.trace.line</field>
        <field name="arch" type="xml">
            <tree>
               <field name="partner_id"/>
               <field name="street"/>
               <field name="district"/>
               <field name="product_id"/>
               <field name="product_qty"/>
               <field name="remark"/>
            </tree>
        </field>
    </record>

 


    <menuitem
    id="shippping_trace_line_menu"
    name="Movimientos de Despacho"
    action="shipping_trace_line_action"
    parent="stock.menu_warehouse_report"
    groups="stock.group_stock_manager"
    sequence="11"/>

</odoo>
