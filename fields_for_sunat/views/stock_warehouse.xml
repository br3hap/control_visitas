<odoo>
  <data>

    <record id="view_warehouse_tree_inherit" model="ir.ui.view">
        <field name="name">view.warehouse.tree.inherit</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="warehouse_annex_code" optional="1"/>
            </xpath>
        </field>
    </record>

    <record id="view_warehouse_inherit" model="ir.ui.view">
        <field name="name">view.warehouse.inherit</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='code']" position="after">
                <field name="warehouse_annex_code"/>
            </xpath>
        </field>
    </record>

  </data>
</odoo>