<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_peb_catalogue_01_tree" model="ir.ui.view">
            <field name="name">peb.catalogue.01.tree</field>
            <field name="model">peb.catalogue.01</field>
            <field name="arch" type="xml">
                <tree string="Catalogue 01">
                    <field name="code"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="view_peb_catalogue_01_form" model="ir.ui.view">
            <field name="name">peb.catalogue.01.form</field>
            <field name="model">peb.catalogue.01</field>
            <field name="arch" type="xml">
                <form string="Catalogue 01">
                    <group>
                        <field name="code"/>
                        <field name="description"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="action_catalogue_01" model="ir.actions.act_window">
            <field name="name">Catalogue 01</field>
            <field name="res_model">peb.catalogue.01</field>
            <field name="view_mode">tree,form</field>
            <!--<field name="search_view_id" ref="view_currency_search"/>-->
        </record>

        <menuitem id="menu_catalogue_01"
                  name="Catalogue 01"
                  action="action_catalogue_01"
                  parent="sunat_catalogue.menu_sunat_catalogue"/>

    </data>
</odoo>

