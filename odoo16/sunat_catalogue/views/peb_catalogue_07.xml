<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_peb_catalogue_07_tree" model="ir.ui.view">
            <field name="name">peb.catalogue.07.tree</field>
            <field name="model">peb.catalogue.07</field>
            <field name="arch" type="xml">
                <tree string="Catalogue 07">
                    <field name="code"/>
                    <field name="description"/>
                    <field name="type_categ"/>
                    <field name="is_onerosa"/>
                </tree>
            </field>
        </record>

        <record id="view_peb_catalogue_07_form" model="ir.ui.view">
            <field name="name">peb.catalogue.07.form</field>
            <field name="model">peb.catalogue.07</field>
            <field name="arch" type="xml">
                <form string="Catalogue 07">
                    <group>
                        <field name="code"/>
                        <field name="description"/>
                        <field name="type_categ"/>
                        <field name="is_onerosa"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="action_catalogue_07" model="ir.actions.act_window">
            <field name="name">Catalogue 07</field>
            <field name="res_model">peb.catalogue.07</field>
            <field name="view_mode">tree,form</field>
            <!--<field name="search_view_id" ref="view_currency_search"/>-->
        </record>

        <menuitem id="menu_catalogue_07"
                  name="Catalogue 07"
                  action="action_catalogue_07"
                  parent="sunat_catalogue.menu_sunat_catalogue"/>

    </data>
</odoo>

