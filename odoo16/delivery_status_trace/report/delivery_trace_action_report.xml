<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	

	<record id="delivery_trace_portrait" model="report.paperformat">
            <field name="name">A4 Custom Service Report</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">40</field>
            <field name="margin_bottom">28</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">35</field>
            <field name="dpi">90</field>
    </record>


	<report id="delivery_status_trace_report_id"
				string="Reporte de Despachos"
				model="shipping.trace"
				report_type="qweb-pdf"
				file="delivery_status_trace.shipping_trace_order"
				name="delivery_status_trace.shipping_trace_order"	
				print_report_name="'Reporte de despacho - %s' % (object.name)"
                paperformat="delivery_trace_portrait"/>				
				
	</data>

</odoo>
