<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="product_template_kardex_form_view" model="ir.ui.view" >
			<field name="name">product.template.kardex.form</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_form_view"></field>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='type']" position="after">
					<field name="type_existence"  options="{'no_create': True, 'no_open': True}"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>