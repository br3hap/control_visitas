<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="account_transfer_view_inherit">
			<field name="name">account.transfer.view.inherit</field>
			<field name="model">account.transfer</field>
			<field name="inherit_id" ref="account_transfer.account_transfer_form" />
			<field name="arch" type="xml">
				
				<xpath expr="//sheet/div[@class='oe_right oe_button_box']"  position="after">
					<h1>
						<label string="Transferencia "/>
						<field name="number" class="oe_inline" readonly="1"/>
					</h1>
					
				</xpath>
				
				<xpath expr="//field[@name='note']"  position="before">
					<group>
						<field name="account_id"
						options="{'no_create_edit':True,'no_quick_create': True,'no_open': True}"/>
						
						<field name="detraccion" readonly="1"/>
					</group>
				</xpath>
			</field>
		</record>
		
		
		<record model="ir.ui.view" id="account_transfer_tree_view_inherit">
			<field name="name">account.transfer.tree.view.inherit</field>
			<field name="model">account.transfer</field>
			<field name="inherit_id" ref="account_transfer.account_transfer_tree" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='date']"  position="before">
					<field name="number"/>
				</xpath>
			</field>
		</record>
		
		<record model="ir.ui.view" id="account_transfer_search_view_inherit">
			<field name="name">account.transfer.search.view.inherit</field>
			<field name="model">account.transfer</field>
			<field name="inherit_id" ref="account_transfer.account_transfer_search" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='ref']"  position="before">
					<field name="number"/>
				</xpath>
			</field>
		</record>
		
	</data>
</openerp>