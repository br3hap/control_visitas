<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>					
         <record id="res_company_guia_remision_inherit" model="ir.ui.view">
			<field name="name">company.view.guia.remision.inherit</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form" />
			<field name="arch" type="xml">			
				<xpath expr="//field[@name='street2']" position="after">
					<field name="district_id" placeholder="District"
						options="{'no_quick_create': true, 'no_create_edit' : true,'no_open': true}">
					</field>
				</xpath>
                <xpath expr="//field[@name='city']" position="replace">
					<field name="city_id" placeholder="City"
						options="{'no_quick_create': true, 'no_create_edit' : true,'no_open': true}">
					</field>
				</xpath>
				<xpath expr="//field[@name='vat']" position="before">
					<field name="identification_type_id"/>
				</xpath>		
			</field>
		</record>		
	</data>
</odoo>